{
  "name": "slide-presentation",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "slidev build presentation.md",
    "dev": "slidev presentation.md",
    "export:md": "slidev export presentation.md --format md --output export/md/presentation",
    "export:pdf": "slidev export presentation.md --output export/pdf/presentation.pdf",
    "export:png": "slidev export presentation.md --format png --output export/png",
    "format": "prettier --write **/*.{json,vue,md,ts}",
    "remote": "slidev presentation.md --remote=your_password",
    "build:downloadable-pdf": "slidev build --download"
  },
  "dependencies": {
    "@slidev/cli": "0.49.0-beta.4",
    "@slidev/theme-default": "latest",
    "vue": "3.4.26"
  },
  "devDependencies": {
    "@slidev/theme-seriph": "0.25.0",
    "@types/node": "20.12.8",
    "playwright-chromium": "1.43.1",
    "prettier": "3.2.5",
    "prettier-plugin-slidev": "1.0.5",
    "typescript": "5.4.5",
    "unocss": "0.59.4"
  },
  "volta": {
    "node": "20.12.2",
    "npm": "10.6.0"
  }
}
